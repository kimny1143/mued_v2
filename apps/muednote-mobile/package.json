{
  "name": "muednote-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean && npm run git:clean-large-files",
    "git:clean-large-files": "git reset HEAD ios/*.bin 2>/dev/null; git reset HEAD android/**/*.bin 2>/dev/null; echo 'Large model files excluded from staging'",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.14",
    "@expo/vector-icons": "^15.0.3",
    "@fugood/react-native-audio-pcm-stream": "^1.1.4",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/slider": "^5.1.1",
    "audio-encoder": "file:./modules/audio-encoder",
    "audio-resampler": "file:./modules/audio-resampler",
    "expo": "~54.0.28",
    "expo-audio": "^1.1.1",
    "expo-auth-session": "^7.0.10",
    "expo-av": "^16.0.8",
    "expo-crypto": "^15.0.8",
    "expo-haptics": "~15.0.8",
    "expo-secure-store": "^15.0.8",
    "expo-share-intent": "^5.1.1",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "^19.2.3",
    "react-native": "0.81.5",
    "react-native-fs": "^2.20.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-svg": "15.12.1",
    "react-native-wheel-pick": "^1.2.6",
    "whisper.rn": "^0.5.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.0",
    "@types/react-native": "~0.73.0",
    "eslint": "^9.0.0",
    "typescript": "~5.7.0"
  }
}
